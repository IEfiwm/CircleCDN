{
  "Routes": [
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "staticenvironment",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/static/{everything}",
      "UpstreamHttpMethod": [
        "GET"
      ],
      "FileCacheOptions": {
        "TtlSeconds": 120
      }
    }
  ]
}